{
  "title": "mediawiki/revision/create",
  "description": "Represents a MW Revision Create event.",
  "$id": "/mediawiki/revision/create/1.2.0",
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "examples": [
